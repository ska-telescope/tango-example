{
  "consumer": {
    "name": "tango device client"
  },
  "provider": {
    "name": "tango device server"
  },
  "interactions": [
    {
      "providerState": "tango device server is running",
      "description": "a read request for calendar_date attribute",
      "request": {
        "method": "read_attribute",
        "arg": "calendar_date"
      },
      "response": {
        "type": "tango.DeviceAttribute",
        "reply": {
          "name": "calendar_date",
          "value": "03/02/0001",
          "quality": "ATTR_VALID",
          "type": "DevString"
        },
        "matchingRules": {
          "$.reply.value": {
            "regex": "\\d+/\\d+/\\d+"
          }
        }
      }
    },
    {
      "providerState": "tango device server is running",
      "description": "a tick command",
      "request": {
        "method": "command_inout",
        "arg": "tick"
      },
      "response": {
        "status": 200,
        "reply": {}
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}

